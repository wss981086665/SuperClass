page{
  background-color: #6A5ACD;
}
.worktip{
  width: 60%;
  height: 120rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  line-height: 120rpx;
  text-align: center;
  font-size: 35px;
  font-weight: 650;
  color: white;
  margin-top: 300rpx;
}
.workid{
  width: 60%;
  height: 120rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  line-height: 120rpx;
  text-align: center;
  font-size: 35px;
  font-weight: 650;
  color: rgb(0, 255, 0);
  box-shadow: inset 0 0 20rpx 0 rgb(161, 152, 218);
  margin-top: 50rpx;
}
.workcon{
  width: 88%;
  font-size: 15px;
  color: white;
  text-align: left;
  margin:  0 auto;
  margin-top: 30rpx;
}
.workbtn{
  width: 50%;
  height: 100rpx;
  position: fixed;
  line-height: 100rpx;
  text-align: center;
  left: 25%;
  color: white;
  bottom: 10%;
  border-radius: 10rpx;
  box-shadow: rgba(182, 182, 182, 0.25) 0px 1px 0px, inset rgba(255,255,255,0.25) 0px 1px 0px, inset rgba(0,0,0,0.25) 0px 0px 0px, inset rgba(255,255,255,0.03) 0px 20px 0px, inset rgba(0,0,0,0.15) 0px -20px 20px, inset rgba(255,255,255,0.05) 0px 20px 20px;
}