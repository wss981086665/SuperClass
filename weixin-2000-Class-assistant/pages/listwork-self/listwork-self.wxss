page{
  background-color: #6A5ACD;
  padding-bottom: 130rpx;
}
.thearea-con{
  width: 96%;
  height: 285rpx;
  padding-top: 10rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  position: relative;
  background-color: white;
  box-shadow: 0 0 15rpx 0 rgb(160, 160, 160);
}
.thearea{
  width: 96%;
  height: 270rpx;
  border: 4rpx solid #6A5ACD;
  background-color: white;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  border-radius: 5rpx;
}
.delete{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 100rpx;
  right: 15rpx;
  animation: demo1 400ms;
  border-radius: 50%;
  overflow: hidden;
  box-shadow: 0 0 20rpx 0 rgb(112, 112, 112);
  background-color: white;
}
.delete-img{
  width: 100%;
  height: 100%;
}
.area-top{
  width: 100%;
  height: 80rpx;
}
.work-tip{
  height: 100%;
  float: left;
  font-size: 18px;
  font-weight: 650;
  margin-left: 15rpx;
  line-height: 80rpx;
  color: rgb(44, 44, 44);
}
.work-code{
  height: 100%;
  float: right;
  font-size: 18px;
  font-weight: 650;
  margin-right: 15rpx;
  line-height: 80rpx;
  color: rgb(0, 228, 0);
}
.area-bottom{
  width: 98%;
  margin-left: 2%;
  height: 188rpx;
  font-size: 15px;
  line-height: 47rpx;
  overflow: hidden;
  color: rgb(44, 44, 44);
}
.juddelete{
  width: 120rpx;
  height: 70rpx;
  position: fixed;
  line-height: 70rpx;
  text-align: center;
  background-color: rgb(240, 0, 0);
  /* box-shadow: 0 0 5px 0 red inset; */
  box-shadow: rgba(255,255,255,0.25) 0px 1px 0px, inset rgba(255,255,255,0.25) 0px 1px 0px, inset rgba(0,0,0,0.25) 0px 0px 0px, inset rgba(255,255,255,0.03) 0px 20px 0px, inset rgba(0,0,0,0.15) 0px -20px 20px, inset rgba(255,255,255,0.05) 0px 20px 20px;
  border-radius: 15rpx;
  color: white;
  bottom: 20px;
  right: 10px;
}
.pageit{
  width: 100%;
  height: 80rpx;
  margin-top: 20rpx;
  /* background-color: blueviolet; */
}
.loadmore{
  width: 80%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
  font-weight: 600;
  color:  rgb(255, 255, 255);
}
.skip{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30px;
  color: white;
  font-weight: 600;
  margin-top: 200rpx;
}
@keyframes demo1{
  from{
    width: 0;
  }
  to{
    width: 100rpx;
  }
}