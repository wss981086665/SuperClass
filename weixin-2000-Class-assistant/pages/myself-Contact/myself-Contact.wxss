/* pages/myself-Contact/myself-Contact.wxss */
page {
  background-color: #6A5ACD;
}
.tip {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  font-size: 20px;
  font-weight: 600;
  margin-top: 20rpx;
  color: rgb(245, 245, 245);
}


.bean {
  width: 94%;
  height: 150rpx;
  line-height: 100rpx;
  margin: 0 auto;
  text-align: center;
  margin-top: 15rpx;
  border-radius: 10rpx;
  border-top-left-radius: 75rpx;
  border-bottom-left-radius: 75rpx;
  background-color: rgba(238, 238, 53, 0.9);
}
.name {
  width: 25%;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  padding-left: 15rpx;
  float: left;
}
.contact {
  width: 65%;
  height: 150rpx;
  line-height: 150rpx;
  padding-left: 15rpx;
  float: left;
  border-left: 1px solid rgb(140, 140, 140);
  text-align: left;
}

.bottom-tip,
.bottom-tip2 {
  width: 100%;
  height: 40rpx;
  text-align: center;
  position: fixed;
  bottom: 60rpx;
  font-size: 12px;
  color: rgb(199, 199, 199);
}
.bottom-tip2 {
  bottom: 20rpx;
}