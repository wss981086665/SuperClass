page{
  background-color: #6A5ACD;
}
.container{
  width: 100vw;
  height: 80vh;
  padding-top: 20vh;
  z-index: 2;
  background-color: #6A5ACD;
}
.search-input{
  width: 0;
  height:0;
  /* width: 90%;
  height: 100rpx; */
  position: fixed;
  background-color: white;
  top: 0;
  left: 0;
}
.search-tip{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 25px;
  text-align: center;
  color: white;
}
.search-view{
  width: 100%;
  height: 150rpx;
  margin-top: 20rpx;
}
.view{
  width: 20%;
  height: 100%;
  margin-left: 4%;
  float: left;
  line-height: 150rpx;
  font-size: 30px;
  border-radius: 10rpx;
  background-color: white;
  text-align: center;
}